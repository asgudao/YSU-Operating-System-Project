<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tjetc.dao.TestNumMapper">
    <sql id="columnName">
        id,
        p_id,
        page_num,
        use_TLB,
        visit_memory,
        visit_TLB,
        handle_losepage,
        input_num,
        create_time
    </sql>
    <resultMap id="baseMap" type="testNum">
        <result column=" id" property="id"></result>
        <result column=" page_num" property="pageNum"></result>
        <result column=" use_TLB" property="useTLB"></result>
        <result column=" TLB_num" property="TLBNum"></result>
        <result column=" visit_memory" property="visitMemory"></result>
        <result column=" visit_TLB" property="visitTLB"></result>
        <result column=" handle_losepage" property="handleLosepage"></result>
        <result column="input_num" property="inputNum"></result>
        <result column="create_time" property="createTime"></result>
    </resultMap>
    <select id="selectById" resultMap="baseMap">
        select <include refid="columnName"></include>
        from test_num
        where id=#{id}
    </select>

</mapper>